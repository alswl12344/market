<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="kr.co.dong.cart">

	<!-- 장바구니 목록 조회 -->
	<select id="CartList" resultType="CartDTO">
		select *
		from cart
		order by cartcode asc
	</select>
	
</mapper>